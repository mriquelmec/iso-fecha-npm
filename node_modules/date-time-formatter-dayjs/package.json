{
  "name": "date-time-formatter-dayjs",
  "version": "1.0.1",
  "description": "NPM package to format date time based on DayJS",
  "repository": "https://github.com/tirth-2001/date-time-formatter-npm.git",
  "author": "Tirth Patel <tirthgpatel.27@gmail.com>",
  "license": "MIT",
  "private": false,
  "main": "lib/index.js",
  "types": "lib/index.d.ts",
  "files": [
    "lib/**/*"
  ],
  "scripts": {
    "dev": "nodemon src/index.ts",
    "build": "tsc",
    "start": "node lib/index.js",
    "format": "prettier --write \"src/**/*.ts\" \"src/**/*.js\"",
    "lint": "tslint -p tsconfig.json",
    "prepare": "yarn build",
    "prepublish": "yarn lint && yarn format"
  },
  "devDependencies": {
    "@types/lodash": "^4.14.188",
    "prettier": "^2.7.1",
    "tslint": "^6.1.3",
    "tslint-config-prettier": "^1.18.0",
    "typescript": "^4.8.4"
  },
  "dependencies": {
    "dayjs": "^1.11.6",
    "lodash": "^4.17.21"
  }
}
